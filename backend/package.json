{
  "name": "forkable-ai-image-app-backend",
  "version": "1.0.0",
  "description": "Backend server for the Forkable AI Image Modification Application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:enhanced": "node server-enhanced.js",
    "dev": "nodemon server.js",
    "dev:enhanced": "nodemon server-enhanced.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:init": "node -e \"require('./src/services/database-service.js').initialize()\"",
    "db:health": "node -e \"require('./src/services/database-service.js').healthCheck().then(console.log)\"",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix"
  },
  "keywords": [
    "ai",
    "image-processing",
    "gemini",
    "express",
    "forkable",
    "nodejs"
  ],
  "author": "Forkable AI App",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "node-fetch": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "pg": "^8.11.0",
    "bcrypt": "^5.1.0",
    "jsonwebtoken": "^9.0.0",
    "express-rate-limit": "^7.1.0",
    "helmet": "^7.1.0",
    "@sentry/node": "^7.77.0",
    "ioredis": "^5.3.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.54.0",
    "eslint-config-standard": "^17.1.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
